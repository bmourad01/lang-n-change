T ~ T.
gjoin T T T.
join T T T T T.
matches T T.
meet T T T T T.
static T.
e --> e.
T <: T ==> T T.
Gamma |- e : T.
Gamma |- e ==> e : T.

%

[C-ARROW]
T1 ~ T1',
T2 ~ T2'
--------------------------------------
(arrow T1 T2) ~ (arrow T1' T2').

[C-DYN]
--------------------------------------
(dyn.) ~ (dyn.).

[C-DYN-T]
--------------------------------------
(dyn.) ~ T.

[C-T-DYN]
--------------------------------------
T ~ (dyn.).

[CC-T-ABS]
[x => T1]Gamma |- e ==> e' : T2
--------------------------------------
Gamma |- (abs T1 (x)e) ==> (abs T1 (x)e') : (arrow T1 T2).

[CC-T-APP]
Gamma |- e1 ==> e1' : M1,
matches M1 (arrow T1_1 T2),
Gamma |- e2 ==> e2' : T1_2,
T1_2 <: T1_1 ==> T1_2' T1_1',
T1_2 ~ T1_2',
T1_1 ~ T1_1'
--------------------------------------
Gamma |- (app e1 e2) ==> (app (gcast e1' M1 (arrow T1_1' T2) L1) (gcast e2' T1_2 T1_2' L2)) : T2.

[CC-T-CAST]
Gamma |- e ==> e' : T1,
T1 <: T2 ==> T1' T2',
T1 ~ T1',
T2 ~ T2'
--------------------------------------
Gamma |- (cast e T2) ==> (cast (gcast e' T1 T1' L3) T2) : T2.

[CC-T-FIX]
Gamma |- e ==> e' : M2,
matches M2 (arrow T_1 T_2),
T_2 <: T_1 ==> T_2' T_1',
T_2 ~ T_2',
T_1 ~ T_1'
--------------------------------------
Gamma |- (fix e) ==> (fix (gcast e' M2 (arrow T_1' T_2') L4)) : T_1.

[CC-T-LET]
Gamma |- e1 ==> e1' : T1,
[x => T1]Gamma |- e2 ==> e2' : T2
--------------------------------------
Gamma |- (let e1 (x)e2) ==> (let e1' (x)e2') : T2.

[CC-T-LETREC]
[x => T1_1]Gamma |- e1 ==> e1' : T1,
[x => T1]Gamma |- e2 ==> e2' : T2,
T1_1 ~ T1
--------------------------------------
Gamma |- (letrec T1 (x)e1 (x)e2) ==> (letrec T1 (x)e1' (x)e2') : T2.

[CC-T-VAR]
member <x, T> Gamma
--------------------------------------
Gamma |- x ==> x : T.

[GJOIN-ARROW]
gjoin T1 T1' T1'',
gjoin T2 T2' T2''
--------------------------------------
gjoin (arrow T1 T2) (arrow T1' T2') (arrow T1'' T2'').

[GJOIN-DYN-T]
--------------------------------------
gjoin (dyn.) T T.

[GJOIN-T-DYN]
--------------------------------------
gjoin T (dyn.) T.

[JOIN-ARROW]
meet T1 T3 T5 T1' T3',
join T2 T4 T6 T2' T4'
--------------------------------------
join (arrow T1 T2) (arrow T3 T4) (arrow T5 T6) (arrow T1' T2') (arrow T3' T4').

[JOIN-ARROW-DYN]
join (arrow T1 T2) (arrow (dyn.) (dyn.)) T1' T2' T3'
--------------------------------------
join (arrow T1 T2) (dyn.) T1' T2' T3'.

[JOIN-DYN]
--------------------------------------
join (dyn.) (dyn.) (dyn.) (dyn.) (dyn.).

[JOIN-DYN-ARROW]
join (arrow (dyn.) (dyn.)) (arrow T1 T2) T1' T2' T3'
--------------------------------------
join (dyn.) (arrow T1 T2) T1' T2' T3'.

[MATCHES-ARROW]
--------------------------------------
matches (arrow T1 T2) (arrow T1 T2).

[MATCHES-DYN-ARROW]
--------------------------------------
matches (dyn.) (arrow (dyn.) (dyn.)).

[MEET-ARROW]
join T1 T7 T9 T1' T7',
meet T2 T8 T10 T2' T8'
--------------------------------------
meet (arrow T1 T2) (arrow T7 T8) (arrow T9 T10) (arrow T1' T2') (arrow T7' T8').

[MEET-ARROW-DYN]
meet (arrow T1 T2) (arrow (dyn.) (dyn.)) T1' T2' T3'
--------------------------------------
meet (arrow T1 T2) (dyn T1 T2) T1' T2' T3'.

[MEET-DYN]
--------------------------------------
meet (dyn.) (dyn.) (dyn.) (dyn.) (dyn.).

[MEET-DYN-ARROW]
meet (arrow (dyn.) (dyn.)) (arrow T1 T2) T1' T2' T3'
--------------------------------------
meet (dyn.) (arrow T1 T2) T1' T2' T3'.

[R-BETA]
--------------------------------------
(app (abs T (x)e) v) --> e{v/x}.

[R-CAST]
--------------------------------------
(cast v T) --> v.

[R-FIX]
--------------------------------------
(fix (abs T (x)e)) --> e{(fix (abs T (x)e))/x}.

[R-LET]
--------------------------------------
(let v (x)e) --> e{v/x}.

[R-LETREC]
--------------------------------------
(letrec T (x)e1 (x)e2) --> (let (fix (abs T (x)e1)) (x)e2).

[S-ARROW]
T11 <: T1 ==> T11' T1',
T2 <: T12 ==> T2' T12'
--------------------------------------
(arrow T1 T2) <: (arrow T11 T12) ==> (arrow T1' T2') (arrow T11' T12').

[S-ARROW-DYN]
(arrow T1 T2) <: (arrow (dyn.) (dyn.)) ==> T1' T2'
--------------------------------------
(arrow T1 T2) <: (dyn.) ==> T1' T2'.

[S-DYN]
--------------------------------------
(dyn.) <: (dyn.) ==> (dyn.) (dyn.).

[S-DYN-ARROW]
(arrow (dyn.) (dyn.)) <: (arrow T1 T2) ==> T1' T2'
--------------------------------------
(dyn.) <: (arrow T1 T2) ==> T1' T2'.

[STATIC-ARROW]
static T1,
static T2
--------------------------------------
static (arrow T1 T2).

[T-ABS]
[x => T1]Gamma |- e : T2
--------------------------------------
Gamma |- (abs T1 (x)e) : (arrow T1 T2).

[T-APP]
Gamma |- e1 : M1,
matches M1 (arrow T1_1 T2),
Gamma |- e2 : T1_2,
T1_2 <: T1_1 ==> T1_2' T1_1',
T1_2 ~ T1_2',
T1_1 ~ T1_1'
--------------------------------------
Gamma |- (app e1 e2) : T2.

[T-CAST]
Gamma |- e : T1,
T1 <: T2 ==> T1' T2',
T1 ~ T1',
T2 ~ T2'
--------------------------------------
Gamma |- (cast e T2) : T2.

[T-FIX]
Gamma |- e : M2,
matches M2 (arrow T_1 T_2),
T_2 <: T_1 ==> T_2' T_1',
T_2 ~ T_2',
T_1 ~ T_1'
--------------------------------------
Gamma |- (fix e) : T_1.

[T-GCAST]
Gamma |- e : T1,
T1 ~ T2
--------------------------------------
Gamma |- (gcast e T1 T2 L) : T2.

[T-LET]
Gamma |- e1 : T1,
[x => T1]Gamma |- e2 : T2
--------------------------------------
Gamma |- (let e1 (x)e2) : T2.

[T-LETREC]
[x => T1_1]Gamma |- e1 : T1,
[x => T1]Gamma |- e2 : T2,
T1_1 ~ T1
--------------------------------------
Gamma |- (letrec T1 (x)e1 (x)e2) : T2.

[T-VAR]
member <x, T> Gamma
--------------------------------------
Gamma |- x : T.

