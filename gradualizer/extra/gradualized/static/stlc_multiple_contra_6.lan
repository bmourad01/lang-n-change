[C-ARROW]
T1 ~ T1',
T2 ~ T2'
--------------------------------------
(arrow T1 T2) ~ (arrow T1' T2').

[C-BOOL]
--------------------------------------
(bool.) ~ (bool.).

[C-DYN]
--------------------------------------
(dyn.) ~ (dyn.).

[C-DYN-T]
--------------------------------------
(dyn.) ~ T.

[C-LIST]
T ~ T'
--------------------------------------
(list T) ~ (list T').

[C-T-DYN]
--------------------------------------
T ~ (dyn.).

[C-TIMES]
T1 ~ T1',
T2 ~ T2'
--------------------------------------
(times T1 T2) ~ (times T1' T2').

[CC-T-ABS]
[x => T1]Gamma |- e ==> e' : T2
--------------------------------------
Gamma |- (abs T1 (x)e) ==> (abs T1 (x)e') : (arrow T1 T2).

[CC-T-APP]
Gamma |- e1 ==> e1' : M1,
matches M1 (arrow T1_1 T2),
Gamma |- e2 ==> e2' : T1_2,
T1_2 ~ T1_1
--------------------------------------
Gamma |- (app e1 e2) ==> (app (gcast e1' M1 (arrow T1_1 T2) L1) (gcast e2' T1_2 T1_1 L2)) : T2.

[CC-T-APP6]
Gamma |- e1 ==> e1' : M3,
matches M3 (arrow M2 T_2),
matches M2 (list T_1),
Gamma |- e2 ==> e2' : M4,
matches M4 (arrow T_3 T_4),
Gamma |- e3 ==> e3' : M5,
matches M5 (list T_5),
T_3 ~ T_2,
T_4 ~ T_1,
T_5 ~ T_1
--------------------------------------
Gamma |- (app6 e1 e2 e3) ==> (app6 (gcast e1' M3 (arrow (list T_1) T_3) L3) (gcast e2' M4 (arrow T_2 T_1) L4) (gcast e3' M5 (list T_1) L5)) : T_4.

[CC-T-CONS]
Gamma |- e1 ==> e1' : T_1,
Gamma |- e2 ==> e2' : M6,
matches M6 (list T_2),
T_1 ~ T,
T_2 ~ T
--------------------------------------
Gamma |- (cons T e1 e2) ==> (cons T (gcast e1' T_1 T L6) (gcast e2' M6 (list T) L7)) : (list T).

[CC-T-FALSE]
--------------------------------------
Gamma |- (ff.) ==> (ff.) : (bool.).

[CC-T-FST]
Gamma |- e ==> e' : M7,
matches M7 (times T1 T2)
--------------------------------------
Gamma |- (fst e) ==> (fst (gcast e' M7 (times T1 T2) L8)) : T1.

[CC-T-HEAD]
Gamma |- e ==> e' : M8,
matches M8 (list T_1),
T_1 ~ T
--------------------------------------
Gamma |- (head T e) ==> (head T (gcast e' M8 (list T) L9)) : T.

[CC-T-IS-NIL]
Gamma |- e ==> e' : M9,
matches M9 (list T_1),
T_1 ~ T
--------------------------------------
Gamma |- (is_nil T e) ==> (is_nil T (gcast e' M9 (list T) L10)) : (bool.).

[CC-T-NIL]
--------------------------------------
Gamma |- (nill T) ==> (nill T) : (list T).

[CC-T-PAIR]
Gamma |- e1 ==> e1' : T1,
Gamma |- e2 ==> e2' : T2
--------------------------------------
Gamma |- (pair e1 e2) ==> (pair e1' e2') : (times T1 T2).

[CC-T-SND]
Gamma |- e ==> e' : M10,
matches M10 (times T1 T2)
--------------------------------------
Gamma |- (snd e) ==> (snd (gcast e' M10 (times T1 T2) L11)) : T2.

[CC-T-TAIL]
Gamma |- e ==> e' : M11,
matches M11 (list T_1),
T_1 ~ T
--------------------------------------
Gamma |- (tail T e) ==> (tail T (gcast e' M11 (list T) L12)) : (list T).

[CC-T-TRUE]
--------------------------------------
Gamma |- (tt.) ==> (tt.) : (bool.).

[CC-T-VAR]
member <x, T> Gamma
--------------------------------------
Gamma |- x ==> x : T.

[GJOIN-ARROW]
gjoin T1 T1' T1'',
gjoin T2 T2' T2''
--------------------------------------
gjoin (arrow T1 T2) (arrow T1' T2') (arrow T1'' T2'').

[GJOIN-BOOL]
--------------------------------------
